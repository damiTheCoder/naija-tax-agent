{
  "name": "cashflow-intelligence-service",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "lint": "eslint .",
    "test": "vitest run",
    "test:unit": "vitest run src/modules/engine",
    "test:integration": "vitest run tests/integration",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "seed": "tsx scripts/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "5.18.0",
    "cors": "2.8.5",
    "dayjs": "1.11.13",
    "dotenv": "16.4.7",
    "express": "4.21.2",
    "helmet": "7.1.0",
    "pino": "9.5.0",
    "pino-http": "10.3.0",
    "uuid": "11.0.3",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/node": "20.17.12",
    "@types/supertest": "2.0.16",
    "eslint": "9.18.0",
    "pino-pretty": "11.3.0",
    "prisma": "5.18.0",
    "supertest": "7.1.1",
    "ts-node": "10.9.2",
    "ts-node-dev": "2.0.0",
    "tsconfig-paths": "4.2.0",
    "tslib": "2.8.1",
    "tsup": "8.2.4",
    "tsx": "4.19.2",
    "typescript": "5.6.3",
    "vitest": "2.1.8"
  }
